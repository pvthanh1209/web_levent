@{
    var controller = ViewContext.RouteData.Values["Controller"].ToString().ToLower();
    //var mems = PTEcommerce.Web.Models.SessionCustomer.GetUser();
}
<!doctype html>
<html lang="en" data-layout="vertical" data-layout-style="default" data-layout-position="fixed" data-topbar="light" data-sidebar="dark" data-sidebar-size="lg" data-layout-width="fluid">
<head>

    <meta charset="utf-8" />
    <title>Trang admin</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta content="Quản lý shop quần áo" name="description" />
    <meta content="Themesbrand" name="author" />
    <!-- App favicon -->
    <link rel="shortcut icon" href="~/Areas/Admin/assets/images/favicon.ico">
    <!-- plugin css -->
    <link href="~/Areas/Admin/assets/libs/jsvectormap/css/jsvectormap.min.css" rel="stylesheet" />
    <script src="~/Areas/Admin/assets/js/layout.js"></script>
    <!-- Bootstrap Css -->
    <link href="~/Areas/Admin/assets/bootstrap-table/bootstrap-table.min.css" rel="stylesheet" />
    <link href="~/Assets/assets/css/bootstrap.min.css" rel="stylesheet" />
    <!-- Icons Css -->
    <link href="~/Areas/Admin/assets/css/bootstrap.min.css" rel="stylesheet" />
    <!-- App Css-->
    <link href="~/Areas/Admin/assets/css/icons.min.css" rel="stylesheet" />
    <link href="~/Areas/Admin/assets/css/app.min.css" rel="stylesheet" />
    <!-- custom Css-->
    <link href="~/Areas/Admin/assets/jquery-confirm/css/jquery-confirm.css" rel="stylesheet" />
    <link href="~/Areas/Admin/assets/css/custom.min.css" rel="stylesheet" />
    <link href="~/Areas/Admin/assets/toastr/toastr.min.css" rel="stylesheet" />

</head>

<body>

    <!-- Begin page -->
    <div id="layout-wrapper">

        @Html.Partial("_header")
        <!-- ========== App Menu ========== -->
        @Html.Partial("_menu")

        <!-- ============================================================== -->
        <!-- Start right Content here -->
        <!-- ============================================================== -->
        <div class="main-content">

            <div class="page-content">
                <div class="container-fluid">
                    @RenderBody()

                </div>
                <!-- container-fluid -->
            </div>
            <!-- End Page-content -->

            <footer class="footer">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-sm-6">
                            <script>document.write(new Date().getFullYear())</script> © Velzon.
                        </div>
                        <div class="col-sm-6">
                            <div class="text-sm-end d-none d-sm-block">
                                Design & Develop by TPV
                            </div>
                        </div>
                    </div>
                </div>
            </footer>
        </div>
        <!-- end main content-->

    </div>
    <!-- JAVASCRIPT -->
    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <script src="~/Areas/Admin/assets/libs/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/Areas/Admin/assets/libs/simplebar/simplebar.min.js"></script>
    <script src="~/Areas/Admin/assets/libs/node-waves/waves.min.js"></script>
    <script src="~/Areas/Admin/assets/libs/feather-icons/feather.min.js"></script>
    <script src="~/Areas/Admin/assets/js/pages/plugins/lord-icon-2.1.0.js"></script>
    <!-- Vector map-->
    <script src="~/Areas/Admin/assets/libs/jsvectormap/js/jsvectormap.min.js"></script>
    <!-- Dashboard init -->
    <script src="~/Areas/Admin/assets/js/pages/dashboard-analytics.init.js"></script>
    <!-- App js -->
    <script src="~/Areas/Admin/assets/jquery-confirm/js/jquery-confirm.js"></script>
    <script src="~/Areas/Admin/assets/libs/moment/moment.js"></script>
    <script src="~/Areas/Admin/assets/js/app.js"></script>
    <script src="~/Areas/Admin/assets/toastr/toastr.min.js"></script>
    <script src="~/Areas/Admin/assets/bootstrap-table/bootstrap-table.min.js"></script>
    <script src="~/Areas/Admin/assets/controllers/accounting.js"></script>
    <script src="~/Areas/Admin/assets/controllers/@(controller).js?v=@DateTime.Now.ToString("yyyyMMddHHmmss")"></script>
    @RenderSection("scripts", required: false)
</body>
</html>

